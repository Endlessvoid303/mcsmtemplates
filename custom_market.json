    {
      "title": "Minecraft Purpur Latest Version",
      "language": "en_us",
      "platform": "Linux",
      "description": "a minecraft server running on purpur and the latest version",
      "image": "https://raw.githubusercontent.com/Endlessvoid303/mcsmtemplates/main/VoidHost.png",
      "gameType": "Minecraft",
      "category": "Latest Version",
      "runtime": "Docker + Java 17+",
      "hardware": "RAM 8G+",
      "size": "",
      "targetLink": "",
      "author": "endlessvoid_303",
      "tags": [
        "Docker"
      ],
      "setupInfo": {
        "type": "minecraft/java",
        "startCommand": "",
        "stopCommand": "stop",
        "processType": "docker",
        "updateCommand": "",
        "ie": "utf-8",
        "oe": "utf-8",
        "docker": {
          "image": "itzg/minecraft-server",
          "ports": [
            "{mcsm_port1}:25565/tcp"
          ],
          "memory": 8192,
          "cpuUsage": 200,
          "env": [
            "EULA=TRUE",
            "MAX_MEMORY=8g",
            "TYPE=PURPUR",
            "VERSION=LATEST",
            "ENABLE_AUTOSTOP=TRUE",
            "AUTOSTOP_TIMEOUT_EST=300",
            "AUTOSTOP_TIMEOUT_INIT=600",
            "TZ=CET",
            "ICON=https://raw.githubusercontent.com/Endlessvoid303/mcsmtemplates/main/VoidHost.png",
            "MOTD=Running %env:TYPE% version %env:VERSION%"
          ],
          "networkMode": "bridge",
          "workingDir": "/data/"
        }
      }
    },
